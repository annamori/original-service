<!DOCTYPE html>
<html>
  <head>
    <title>Liver</title>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div class="container">
       <% flash.each do |message_type, message| %> 
      <div class="alert alert-<%= message_type %>"><%= message %></div>
    <% end %> 
      <div class="container">
      <%= yield %>
    </div>
  </body>
</html>